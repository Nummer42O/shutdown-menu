<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="text-viewer">
	<schema id="@_application_id@" path="@_application_id_path@">
    <key name="timeout-length" type="i">
      <summary>Seconds until timeout</summary>
      <description>The ammount of seconds that need to count down before the default action gets applied.</description>
      <range min='10'/>
      <default>60</default>
    </key>
    <key name="timeout-interval" type="i">
      <summary>Seconds between countdown updates</summary>
      <description>The ammount of seconds it takes to update the displayed remaining timer value.</description>
      <range min='1'/>
      <default>10</default>
    </key>
    <key name="grace-period" type="i">
      <range min='0'/>
      <default>0</default>
    </key>
	</schema>
</schemalist>
